{
	"compilerOptions": {
		"outDir": "./dist/",
		"sourceMap": true,
		"noImplicitAny": true,
		"module": "es6",
		"moduleResolution": "node",
		"target": "es5",
		"jsx": "react",
		"allowJs": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"baseUrl": "./",
		"paths": {
			"*": [
				"node_modules/*",
				"src/types/*"
			]
		}
	},
	"include": ["./src/**/*"],
}
